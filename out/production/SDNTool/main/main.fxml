<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.MainController">
  <children>
      <Label text="Controller Adresse " />
      <TextField id="sourceAddressText" />
      <Label layoutX="22.0" layoutY="22.0" text="Ziel Adresse" />
      <TextField id="destinationAddressText" layoutX="22.0" layoutY="57.0" />
      <Label layoutX="22.0" layoutY="108.0" text="Out Port" />
      <TextField id="outPortText" layoutX="22.0" layoutY="143.0" />
    <Button fx:id="activateFlowButton" alignment="CENTER" cancelButton="false" maxHeight="-1.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Flow aktivieren" textAlignment="CENTER">
      <VBox.margin>
        <Insets top="32.0" />
      </VBox.margin>
    </Button>
    <Separator prefWidth="200.0">
      <VBox.margin>
        <Insets top="12.0" />
      </VBox.margin>
    </Separator>
    <Slider fx:id="bandwidthSlider" maxWidth="1.7976931348623157E308" minorTickCount="5" prefWidth="-1.0" showTickLabels="true" showTickMarks="true" snapToTicks="true">
      <VBox.margin>
        <Insets bottom="12.0" top="12.0" />
      </VBox.margin>
    </Slider>
  </children>
   <padding>
      <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
   </padding>
</VBox>
